<div class="container">
  <h3>Vos rendez-vous</h3>
  <% @consultations.each do |consultation| %>
    <div class="row">
      <%= link_to new_consultation_diagnostic_path(consultation) do %>
        <div class="card">
          <h5 class="card-title"><%= consultation.patient.name %></h5>
          <p class="card-body"><%= consultation.starts_at %></p>
        </div>
      <% end %>
    </div>
  <% end %>
</div>
